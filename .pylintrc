[MASTER]
py-version=3.12
jobs=0
ignore=CVS,.git,__pycache__,.ansible,build,dist,*.egg-info,venv
ignore-patterns=^\.#,.*_cache.*,.*\.ansible.*
init-hook='import sys; sys.path.append("plugins"); sys.path.append("plugins/module_utils")'
persistent=yes
extension-pkg-allow-list=
recursive=no

[MESSAGES CONTROL]
disable=
    wrong-import-position,
    protected-access,
    arguments-differ,
    arguments-renamed,
    signature-differs,
    too-many-ancestors,
    unused-argument,
    duplicate-code,
    import-error,
    no-name-in-module,
    too-many-branches,
    too-many-statements,
    fixme

enable=
    use-symbolic-message-instead,
    useless-suppression

[REPORTS]
output-format=colorized
reports=no
score=yes

[BASIC]
good-names=i,j,k,ex,_,id,ip,fd,rv
argument-rgx=[a-z_][a-z0-9_]{1,30}$
attr-rgx=[a-z_][a-z0-9_]{1,30}$
class-attribute-rgx=([A-Za-z_][A-Za-z0-9_]{1,30}|(__.*__))$
class-rgx=[A-Z_][a-zA-Z0-9]+$
const-rgx=(([A-Z_][A-Z0-9_]*)|(__.*__))$
function-rgx=[a-z_][a-z0-9_]{1,40}$
method-rgx=[a-z_][a-z0-9_]{1,50}$
module-rgx=(([a-z_][a-z0-9_]*)|([A-Z][a-zA-Z0-9]+))$
variable-rgx=[a-z_][a-z0-9_]{1,30}$
include-naming-hint=yes
no-docstring-rgx=^_
docstring-min-length=10

[FORMAT]
max-line-length=100
single-line-if-stmt=no
max-module-lines=1000
indent-string='    '
indent-after-paren=4

[LOGGING]
logging-format-style=new
logging-modules=logging

[MISCELLANEOUS]
notes=FIXME,XXX,TODO,HACK,REFACTOR

[SIMILARITIES]
min-similarity-lines=6
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=yes
ignore-signatures=yes

[TYPECHECK]
contextmanager-decorators=contextlib.contextmanager
generated-members=
ignore-mixin-members=yes
ignored-classes=optparse.Values,thread._local,_thread._local
missing-member-hint=yes
ignored-modules=

[VARIABLES]
additional-builtins=
allow-global-unused-variables=yes
callbacks=cb_,_cb
dummy-variables-rgx=_+$|(_[a-zA-Z0-9_]*[a-zA-Z0-9]+?$)|dummy|^ignored_|^unused_
ignored-argument-names=_.*|^ignored_|^unused_|args|kwargs
redefining-builtins-modules=six.moves,past.builtins,future.builtins,builtins,io

[CLASSES]
defining-attr-methods=__init__,__new__,setUp,__post_init__
valid-classmethod-first-arg=cls
valid-metaclass-classmethod-first-arg=mcs
exclude-protected=_asdict,_fields,_replace,_source,_make

[DESIGN]
max-args=7
max-attributes=12
max-bool-expr=5
max-branches=15
max-locals=20
max-parents=7
max-public-methods=25
max-returns=8
max-statements=60
min-public-methods=1

[IMPORTS]
deprecated-modules=optparse,tkinter.tix
ext-import-graph=
int-import-graph=
known-third-party=
allow-wildcard-with-all=yes

[EXCEPTIONS]
overgeneral-exceptions=builtins.BaseException,builtins.Exception

[STRING]
check-quote-consistency=no
